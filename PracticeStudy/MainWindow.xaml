<Window x:Class="PracticeStudy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticeStudy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="345*"/>
           
        </Grid.RowDefinitions>
        <Rectangle Fill="#FFB5D5CA"/>
        <Image HorizontalAlignment="Left" ></Image>
        <TextBlock x:Name="TitlePage" FontSize="25" HorizontalAlignment="Center" Style="{StaticResource TxtBlock}">Магазин продуктов</TextBlock>
        <Button x:Name="BackBtn" HorizontalAlignment="Right" Margin="100 0" Style="{StaticResource Button}" Click="BackBtn_Click">Назад</Button>
        <Button x:Name="ExitBtn" HorizontalAlignment="Right" Margin="10 0" Style="{StaticResource Button}" Click="ExitBtn_Click">Выход</Button>
        
        <Frame Grid.Row="1" Name="FrameMain" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
